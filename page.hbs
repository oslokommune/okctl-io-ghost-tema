{{!< default}}
{{! The tag above means - insert everything in this file into the {body} of the default.hbs template }}
<div class="main">
    <div class="container">
        <div class="row">
            <div class="col-lg-10 offset-lg-1">
                {{#post}}
                <div class="single-post-wrap">
                    <h1 class="post-title">{{title}}</h1>
                    <div class="post-content">
                        {{content}}
                    </div>
                </div>
                {{/post}}
            </div>
        </div>
    </div>
    {{> contact-cta}}
</div>